@model IEnumerable<AlphaABSV2.Models.EventRecord>


<table class="table">
    <tr>
    <th>
        @Html.DisplayNameFor(model => model.BookingForm.BookingRef)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.BookingForm.PartyName)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.StartTime)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.BookingForm.Email)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.EventNumber)
