            @Html.DisplayFor(modelItem => item.BookingForm.AmountTaken)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BookingForm.BalanceRemaining)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.BookingFormID }) |
